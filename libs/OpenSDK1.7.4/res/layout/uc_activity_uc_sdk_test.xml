<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:orientation="vertical" >

    <Button
        android:id="@+id/register_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="注册界面" />

    <Button
        android:id="@+id/login_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="登陆界面" />

    <Button
        android:id="@+id/reqQuickRegister"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="请求注册" />

    <Button
        android:id="@+id/reqToken"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="请求登录" />

    <Button
        android:id="@+id/account_logout_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="请求退出" />

    <Button
        android:id="@+id/account_isLogin_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="account_test" />
    
    <Button
        android:id="@+id/account_result_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="account_result_test" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView" />

</LinearLayout>