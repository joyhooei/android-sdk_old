<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="${PACKAGE_NAME}"
    android:versionCode="${VERSION_CODE}"
    android:versionName="${VERSION_NAME}" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.WRITE_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
	<uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" /> 

    <application
        android:allowBackup="true" 
        android:label="@string/app_name" 
        android:icon="@drawable/ic_launcher"
        android:name="org.yunyue.app" >

        <activity
            android:name="org.yunyue.poem"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- activity for sdk all begin -->
        <!-- activity for sdk core -->
        
        <activity
            android:name="com.nearme.gamecenter.open.core.WebActivity"
            android:configChanges="keyboardHidden|orientation|screenSize" >
        </activity>
        <activity
            android:name="com.nearme.gamecenter.open.core.LogoActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <activity
            android:name="com.nearme.gamecenter.open.core.LoginActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/Theme_Dialog_Custom"  >
        </activity>
        <activity
            android:name="com.nearme.gamecenter.open.core.WebViewBarActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <activity
            android:name="com.nearme.gamecenter.open.core.ActiveDetailActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        
        <activity
            android:name="com.nearme.gamecenter.open.core.KebiChargeActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTask"
            android:theme="@style/nmgc_activitynotfloat" >
        </activity>
        <activity
            android:name="com.nearme.gamecenter.open.core.KebiNormalPayActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTask"
            android:theme="@style/nmgc_activitynotfloat" >
        </activity>
        <activity
            android:name="com.nearme.gamecenter.open.core.KebiRealPayActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTask"
            android:theme="@style/nmgc_activitynotfloat" >
        </activity>
        <activity
            android:name="com.nearme.gamecenter.open.core.KebiRatePayActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTask"
            android:theme="@style/Theme_Dialog_Custom" >
        </activity>
        
        <activity
            android:name="com.nearme.gamecenter.open.core.KebiFixedPayActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTask"
            android:theme="@style/Theme_Dialog_Custom" >
        </activity>
        
        <activity
            android:name="com.nearme.gamecenter.open.core.RegistActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/Theme_Dialog_Custom" >
        </activity>
        
        <activity
            android:name="com.nearme.gamecenter.open.core.AccountLoginActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/Theme_Dialog_Custom" >
        </activity>
        
        <activity
            android:name="com.nearme.gamecenter.open.core.FastRegActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/Theme_Dialog_Custom" >
        </activity>
        
        <activity
            android:name="com.nearme.gamecenter.open.core.PhoneRegActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/Theme_Dialog_Custom" >
        </activity>
        
        <activity
            android:name="com.nearme.gamecenter.open.core.OneKeyRegSuccessActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/Theme_Dialog_Custom"  >
        </activity>
        
        <activity
            android:name="com.nearme.gamecenter.open.core.NormalPhoneRegActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/Theme_Dialog_Custom"  >
        </activity>
        
        <activity
            android:name="com.nearme.gamecenter.open.core.NormalRegActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/Theme_Dialog_Custom"  >
        </activity>
    
        <!-- activity for sdk upgrade -->
        <activity
            android:name="com.oppo.upgrade.activity.ForceUpgradeDialog"
            android:configChanges="keyboardHidden|orientation|locale"
            android:launchMode="singleTask"
            android:screenOrientation="nosensor"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.oppo.upgrade.activity.UpgradeDialog"
            android:configChanges="keyboardHidden|orientation|locale"
            android:launchMode="singleTask"
            android:screenOrientation="nosensor"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.oppo.upgrade.activity.MaintainActivity"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTask"
            android:screenOrientation="nosensor"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        
        
        <!-- activity for sdk charge begin -->
        <!-- 支付相关的activity -->
         <activity
            android:name="com.nearme.wappay.activity.BaseClientActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true" >
        </activity>
        <activity
            android:name="com.nearme.wappay.activity.BasePluginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true" >
        </activity>
        <activity
            android:name="com.nearme.wappay.activity.BasePayActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:theme="@style/NearMePayActivityNotFloat"
            android:windowSoftInputMode="stateHidden|adjustResize" >
        </activity>
        <activity
            android:name="com.nearme.wappay.activity.BaseRechargeActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:theme="@style/NearMePayActivityNotFloat"
            android:windowSoftInputMode="stateHidden|adjustResize" >
        </activity>
        <activity
            android:name="com.nearme.wappay.activity.StartPayActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:theme="@style/NearMePayActivityNotFloat"
            android:windowSoftInputMode="stateHidden|adjustResize" >
        </activity>
        <activity
            android:name="com.nearme.wappay.activity.AlipayPluginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.nearme.wappay.activity.UniPayActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.nearme.wappay.activity.UPPayActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.nearme.wappay.activity.TenPayPluginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:theme="@style/NearMePayActivityNotFloat" />
        <activity
            android:name="com.nearme.wappay.activity.NativeSmsActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.nearme.wappay.activity.DMBillingPayActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
		<activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" >
        </activity>
        <activity
            android:name="com.alipay.sdk.auth.AuthActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" >
        </activity>
        <activity
            android:name="com.unionpay.uppay.PayActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:screenOrientation="portrait" />
        <!-- 支付相关的activity -->

        <!-- activity for sdk charge end --> 
        <!-- activity for sdk all end !!! -->
        <meta-data android:name="sdktype" android:value="${SDKTYPE}"/>
        <meta-data android:name="YYPackageName" android:value="${YY_PACKAGE_NAME}" />
        <meta-data android:name="appid" android:value="${APPID}"/>
    </application>

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true" />

</manifest>
