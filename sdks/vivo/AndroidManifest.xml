                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.bbk.payment.PaymentRechargeActivity"
            android:screenOrientation="portrait" android:theme="@android:style/Theme.NoTitleBar" >
